@model IEnumerable<WebApplication1.Models.PlayerViewModel>
@{
    ViewBag.Title = "MyTeam";
    Layout = "~/Views/Shared/_LayoutAfterLogin.cshtml";
}

<h2>Mano komanda</h2>
<table class="invisibleTableBorders">
    <tr>

        <th>Vardas</th>
        <th>Taškai</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.Name</th>
            <th>@item.Points</th>
        </tr>
    }

</table>